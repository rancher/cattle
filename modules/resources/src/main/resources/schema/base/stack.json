{
  "resourceFields": {
    "name": {
      "required": true,
      "validChars": "a-zA-Z0-9-",
      "minLength": 1,
      "maxLength": 63,
      "attributes": {
        "scheduleUpdate": true
      }
    },
    "allowSystemRole": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "environment": {
      "type": "map[string]"
    },
    "answers": {
      "type": "map[json]"
    },
    "templates": {
      "type": "map[string]",
      "nullable": true
    },
    "previousExternalId": {
      "type": "string",
      "nullable": true
    },
    "previousEnvironment": {
      "type": "map[string]",
      "nullable": true
    },
    "startOnCreate": {
      "type": "boolean",
      "nullable": true
    },
    "outputs": {
      "type": "map[string]",
      "nullable": true
    },
    "system": {
      "type": "boolean",
      "nullable": false,
      "default": false
    },
    "upgrade": {
      "type": "kubernetesStackUpgrade"
    },
    "serviceIds": {
      "type": "array[reference[service]]",
      "nullable": true
    }
  },
  "resourceActions": {
    "upgrade": {
      "input": "stackUpgrade",
      "output": "stack"
    },
    "addoutputs": {
      "input": "addOutputsInput",
      "output": "stack"
    },
    "exportconfig": {
      "input": "composeConfigInput",
      "output": "composeConfig"
    },
    "activateservices": {
      "output": "stack"
    },
    "deactivateservices": {
      "output": "stack"
    }
  }
}
