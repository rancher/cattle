  "resourceActions": {
    "logs": {
      "input": "containerLogs",
      "output": "hostAccess",
      "attributes": {
        "states": [
          "running",
          "stopped",
          "stopping"
        ]
      }
    },
    "converttoservice": {
      "output": "service"
    },
    "upgrade": {
      "input": "containerUpgrade",
      "output": "revision",
      "attributes": {
        "defaults": "false"
      }
    },
    "execute": {
      "input": "containerExec",
      "output": "hostAccess",
      "attributes": {
        "state": "running"
      }
    },
    "proxy": {
      "input": "containerProxy",
      "output": "hostAccess",
      "attributes": {
        "state": "running"
      }
    }
