{
    "resourceFields": {
        "name":{
            "required": true,
            "validChars": "a-zA-Z0-9-",
            "minLength": 1,
             "attributes" : {
                "scheduleUpdate" : true
            }
        },
        "dockerCompose":{
            "type": "string"
        },
        "rancherCompose":{
            "type": "string"
        },
        "environment":{
            "type": "map[string]"
        }
    },
    "resourceActions" : {
        "upgrade" : {
            "input" : "environmentUpgrade",
            "output" : "environment"
        },
        "exportconfig" : {
            "input" : "composeConfigInput",
            "output" : "composeConfig"
        },
        "activateservices" : {
        },
        "deactivateservices" : {
        }
    }
}
